{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"

        },
        {
           "type": "package",
           "package": {
           "name": "d3/d3",
           "version": "v3.5.17",
           "type": "drupal-library",
           "dist": {
            "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
            "type": "zip"
                }
            }
         },
         {
          "type": "package",
          "package": {
          "name": "components/highlightjs",
          "version": "9.7.0",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
            "type": "zip"
                   }
                   }
          }

    ],
    "require": {
        "php": ">=5.6",
        "balbuf/drupal-libraries-installer": "^1.0",
        "components/highlightjs": "9.7.*",
        "composer/installers": "1.5.0",
        "cweagans/composer-patches": "^1.6.5",
        "d3/d3": "3.5.*",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_content_notification": "^3.5",
        "drupal/admin_toolbar": "^1.26",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.4",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/block_class": "^1.0",
        "drupal/bootstrap_barrio": "^4.21",
        "drupal/bootstrap_library": "^1.11",
        "drupal/bootstrap_paragraphs": "^2.0@beta",
        "drupal/captcha": "^1.0@beta",
        "drupal/computed_field": "^2.0@beta",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/core": "8.7.2",
        "drupal/csv_importer": "^1.4",
        "drupal/ctools": "^3.2",
        "drupal/datetime_extras": "1.x-dev",
        "drupal/devel": "2.x-dev",
        "drupal/ds": "^3.2",
        "drupal/email_registration": "^1.0",
        "drupal/entity_block": "^1.0@beta",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/fapi_validation": "^2.0@alpha",
        "drupal/faqfield": "^1.1",
        "drupal/feeds": "^3.0",
        "drupal/field_display_label": "^1.0",
        "drupal/field_group": "^3.0",
        "drupal/fontawesome": "^2.13",
        "drupal/force_password_change": "^1.3",
        "drupal/google_analytics": "^3.0",
        "drupal/imce": "^1.7",
        "drupal/mailgun": "^1.0@beta",
        "drupal/menu_block": "^1.5",
        "drupal/menu_breadcrumb": "^1.8",
        "drupal/migrate_plus": "^4.1",
        "drupal/migrate_tools": "^4.1",
        "drupal/migrate_upgrade": "^3.0",
        "drupal/nodeaccess": "^1.0@beta",
        "drupal/override_node_options": "^2.4",
        "drupal/page_manager": "^4.0@beta",
        "drupal/page_title_visibility": "^1.1",
        "drupal/panels": "^4.3",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.3",
        "drupal/profile": "^1.0@RC",
        "drupal/recaptcha": "^2.4",
        "drupal/rules": "^3.0@alpha",
        "drupal/scheduler": "^1.0",
        "drupal/simple_mail": "^1.1",
        "drupal/single_datetime": "1.x-dev",
        "drupal/smart_trim": "^1.1",
        "drupal/smtp": "^1.0@beta",
        "drupal/subpathauto": "^1.0@RC",
        "drupal/superfish": "^1.3",
        "drupal/tamper": "1.x-dev",
        "drupal/taxonomy_import": "^1.0",
        "drupal/token": "^1.5",
        "drupal/token_custom": "^1.0@alpha",
        "drupal/token_filter": "^1.1",
        "drupal/typed_data": "1.x-dev",
        "drupal/viewfield": "^3.0@alpha",
        "drupal/views_accordion": "^1.2",
        "drupal/views_bootstrap": "^4.1",
        "drupal/views_bulk_operations": "^2.5",
        "drupal/views_conditional": "^1.0",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/viewsreference": "^1.4",
        "drupal/webform": "^5.1",
        "drupal/workflow": "^1.1",
        "drush/drush": "^9.0.0",
        "egulias/email-validator": "2.1.7",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-core-require-dev": "8.7.2",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-libraries": {
            "datetimepicker": "https://github.com/xdan/datetimepicker/archive/2.5.20.zip"
         },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
